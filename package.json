{
  "name": "word-map",
  "description": "Fast way to convert fixed length text",
  "repository": "https://github.com/asiloisad/pulsar-word-map",
  "keywords": [
    "word",
    "map",
    "greek",
    "letter",
    "snippets"
  ],
  "version": "1.0.4",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "activationCommands": {
    "atom-text-editor": [
      "word-map:auto",
      "word-map:selected",
      "word-map:fixed-1",
      "word-map:fixed-2",
      "word-map:fixed-3",
      "word-map:fixed-4",
      "word-map:fixed-5"
    ]
  }
}
